{
  "name": "auto-route",
  "version": "1.0.0",
  "packageManager": "pnpm@7.1.0",
  "engines": {
    "node": ">=14",
    "pnpm": ">=7"
  },
  "keywords": [
    "Vue",
    "Vue Router",
    "routing",
    "generator"
  ],
  "license": "MIT",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "pnpm run build-core && pnpm run build-webpack",
    "build-webpack": "pnpm -r --filter webpack-plugin run build",
    "build-core": "pnpm -r --filter core run build",
    "test-webpack": "pnpm -r --filter webpack-plugin run test-update",
    "test": "vitest",
    "lint": "eslint packages --fix --ext .ts"
  },
  "devDependencies": {
    "@types/node": "^15.14.9",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.27.2"
  },
  "peerDependencies": {
    "vue-demi": "^0.13.11"
  },
  "workspaces": [
    "packages/*"
  ]
}
