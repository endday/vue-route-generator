{
  "name": "auto-route",
  "version": "1.0.0",
  "packageManager": "pnpm@7.1.0",
  "engines": {
    "node": ">=14",
    "pnpm": ">=7"
  },
  "keywords": [
    "Vue",
    "Vue Router",
    "routing",
    "generator"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "pnpm -r --filter=./packages/core run build"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "/test/.+\\.spec\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "test/tsconfig.json"
      }
    }
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^15.14.9",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "workspaces": [
    "packages/*"
  ]
}
